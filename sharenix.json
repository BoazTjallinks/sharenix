{
    "DefaultFileUploader": "hnng.moe (file upload)",
    "DefaultImageUploader": "imgur.com",
    "DefaultUrlShortener": "waa.ai",
    "XineramaHead": 0,
    "NotificationTime": 30,

    "Services": [
        {
            "Name": "waa.ai",
            "RequestType": "GET",
            "RequestURL": "https://api.waa.ai/shorten",
            "Arguments": {
                "url": "$input$"
            },
            "ResponseType": "Text",
            "URL": "$json:data.url$",
            "DeletionURL": "$json:data.delete_link$"
        },
        {
            "Name": "hnng.moe (file upload)",
            "RequestType": "POST",
            "RequestURL": "http://www.hnng.moe/upload_d.php",
            "FileFormName": "file",
            "ResponseType": "Text",
            "URL": "$json:url$",
            "DeletionURL": "$json:deletelink$"
        },
        {
            "Name": "imgur.com",
            "RequestType": "POST",
            "Headers": {
                "Authorization": "Client-ID b972ecca954f246"
            },
            "RequestURL": "https://api.imgur.com/3/image",
            "FileFormName": "image",
            "Arguments": {
                "type": "file"
            },
            "ResponseType": "Text",
            "URL": "$json:data.link$",
            "DeletionURL": "https://imgur.com/delete/$json:data.deletehash$"
        },
        {
            "Name": "owncloud (demo)",
            "RequestType": "PUT",
            "RequestURL": "https://demo.owncloud.org/remote.php/webdav/$Y$-$M$-$D$_$h$-$m$-$s$_$n$$extension$",
            "Username": "test",
            "Password": "test",
            "ResponseType": "RedirectionURL"
        }
    ]
}
